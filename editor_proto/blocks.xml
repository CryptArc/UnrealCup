<blocks>
    <stdBlock name="for">
        <input type="number">start</input>
        <input type="number">end</input>
        <input type="number">increment</input>
        <output type="flow">do</output>
    </stdBlock>
    <stdBlock name="if">
        <input type="text">expression</input>
        <output type="flow">then</output>
        <output type="flow">else</output>
    </stdBlock>
    <stdBlock name="while">
        <input type="text">expression</input>
        <output type="flow">do</output>
    </stdBlock>
    <stdBlock name="setVariable">
        <input type="text">name</input>
        <input type="any">value</input>
    </stdBlock>
    <stdBlock name="getVariable">
        <input type="text">name</input>
        <output type="any">value</output>
    </stdBlock>
    <block name="moveTo">
        <input type="number">xPosition</input>
        <input type="number">yPosition</input>
    </block>
	<block name="Stamina">
        <output type="number">Stamina</output>
    </block>
	<block name="has Ball">
        <output type="number">has Ball</output>
    </block>
    <block name="kick">
        <input type="number">strength</input>
		<input type="number">xDirection</input>
        <input type="number">yDirection</input>
    </block>
    <block name="getBallLocation">
        <output type="number">xPosition</output>
        <output type="number">yPosition</output>
    </block>
    <block name="getPlayerLocation">
        <output type="number">xPosition</output>
        <output type="number">yPosition</output>
    </block>
</blocks>